{
    "workbench.editor.enablePreview": false,
    "workbench.editorAssociations": {
        "*.html": "default"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "window.zoomLevel": 1,
    "maven.excludedFolders": [
        "**/.*",
        "**/node_modules",
        "**/target",
        "**/bin",
        "**/archetype-resources"
    ],
    "gitlens.views.remotes.branches.layout": "list",
    "gitlens.graph.dimMergeCommits": true,
    "vscode-json-editor.theme": "dark",
    "gitlens.graph.minimap.additionalTypes": [
        "localBranches",
        "remoteBranches",
        "stashes"
    ],
    "hediet.vscode-drawio.resizeImages": null,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-21",
            "path": "/Users/chungil/.sdkman/candidates/java/current",
            "default": true
        },
        {
            "name": "JavaSE-19",
            "path": "/Library/Java/JavaVirtualMachines/temurin-19.jdk/Contents/Home"
        }
    ],
    "java.jdt.ls.java.home": "/Users/chungil/.sdkman/candidates/java/current",
    "java.import.gradle.java.home": "/Users/chungil/.sdkman/candidates/java/current",
    "java.compile.nullAnalysis.nonnull": [
        "javax.annotation.Nonnull",
        "org.eclipse.jdt.annotation.NonNull",
        "org.springframework.lang.NonNull"
    ],
    "editor.fontFamily": "Zed Mono light extended, JetBrains Mono light,   Menlo, Monaco, 'Courier New', monospace",
    "editor.inlineSuggest.suppressSuggestions": true,
    "material-icon-theme.languages.associations": {},
    "workbench.colorTheme": "One Monokai",
    "workbench.iconTheme": "lucodear-icons",
    "java.saveActions.organizeImports": true,
    "java.debug.settings.console": "internalConsole",
    "java.import.gradle.version": "",
    "java.import.gradle.home": "",
    "editor.tokenColorCustomizations": {
        "[One Monokai]": {
            "textMateRules": [
                // 	
                {
                    "scope": [
                        "punctuation.definition.annotation.java",
                        "storage.type.annotation.java"
                    ],
                    "settings": {
                        "foreground": "#6e8576",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "comment.line.double-slash.java"
                    ],
                    "settings": {
                        // "foreground": "#b7701e",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "keyword.other.import.java"
                    ],
                    "settings": {
                    "foreground": "#1eb78c",
                    "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "storage.modifier"
                    ],
                    "settings": {
                    "foreground": "#4f822a",
                    "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "storage.modifier.java"
                    ],
                    "settings": {
                    "foreground": "#34ac9c",
                    "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "keyword.operator"
                    ],
                    "settings": {
                    "foreground": "#1eb78c",
                    "fontStyle": "italic bold"
                    }
                },
                {
                    "scope": [
                        "variable.language.this.java",
                        "variable.language.java"
                    ],
                    "settings": {
                    "foreground": "#20B78C",
                    "fontStyle": "italic"
                    }
                },
                {
                    "scope": [
                        "keyword.control"
                    ],
                    "settings": {
                    "foreground": "#ae793c",
                    "fontStyle": "italic"
                    }
                },
                // variable.language.this.java
                // storage.modifier.extends.java
                // storage.modifier.implements.java
                // keyword.operator.assignment.java
                // keyword.operator.arithmetic.java
                // keyword.control.new.java
            
            ]
        },
    },
    "editor.mouseWheelZoom": true,
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.maxLineCount": 3,
    "workbench.colorCustomizations": {
        "editor.background": "#282C33",
        "editorGroupHeader.tabsBackground": "#2F343E",

        "list.inactiveSelectionBackground": "#464B57",
        "list.activeSelectionBackground": "#282C33",
        "list.focusOutline": "#282C33",

        "tab.border": "#464B57",
        "tab.activeBorderTop": "#464B57",
        "tab.lastPinnedBorder": "#464B57",
        "tab.activeBorder": "#464B57",
        "tab.inactiveBackground": "#36383e",
        "tab.hoverBackground": "#2F343E",
        "tab.activeBackground": "#2F343E",
        
        "sideBar.background": "#2F343E",
        "sideBarSectionHeader.background": "#282C33",
        
        "panel.background": "#2F343E",
        "panelSectionHeader.background": "#282C33",

        "debugConsoleInputIcon.foreground": "#7f7979",
        "debugConsole.sourceForeground": "#878678",
        "debugConsole.infoForeground": "#93c1ce",
        "debugConsole.warningForeground": "#d8dd41",
        "debugConsole.errorForeground": "#e74848",

        "terminal.background":"#2F343E",
        "terminal.foreground":"#A5A2A2",
        "terminalCursor.background":"#A5A2A2",
        "terminalCursor.foreground":"#A5A2A2",
        "terminal.ansiBlack":"#090300",
        "terminal.ansiBlue":"#01A0E4",
        "terminal.ansiBrightBlack":"#5C5855",
        "terminal.ansiBrightBlue":"#01A0E4",
        "terminal.ansiBrightCyan":"#B5E4F4",
        "terminal.ansiBrightGreen":"#01A252",
        "terminal.ansiBrightMagenta":"#A16A94",
        "terminal.ansiBrightRed":"#DB2D20",
        "terminal.ansiBrightWhite":"#F7F7F7",
        "terminal.ansiBrightYellow":"#FDED02",
        "terminal.ansiCyan":"#B5E4F4",
        "terminal.ansiGreen":"#01A252",
        "terminal.ansiMagenta":"#A16A94",
        "terminal.ansiRed":"#DB2D20",
        "terminal.ansiWhite":"#A5A2A2",
        "terminal.ansiYellow":"#FDED02",

        "titleBar.inactiveBackground": "#3A414D",
        "titleBar.inactiveForeground": "#e7e7e799",
        "titleBar.activeBackground": "#3A414D",
        "titleBar.activeForeground": "#e7e7e7",
        
        "activityBar.activeBackground": "#74717175",
        "activityBar.background": "#3A414D",

    },
    "editor.minimap.enabled": false,

    "errorLens.fontFamily": "Zed Mono Light",
    "errorLens.fontStyleItalic": true,
    "editor.tabCompletion": "on",
    "editor.renderWhitespace": "trailing",
    "errorLens.delayMode": "debounce",

}
